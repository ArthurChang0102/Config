alias python='python3'
alias pip='pip3'
alias activate='source ./env/bin/activate'
alias pip-make-list='pip freeze > ./requirements.txt'
alias venv='python -m venv env && activate'

alias l='ls -lF'

alias pp='ping 8.8.8.8'
alias pd='ping fb.me'

alias g='git'
alias gl='git l'
alias gd='git diff'
alias gs='git status'
alias gp='git push'
alias gpp='git pull'
alias ga='git add'
alias gcl='git clone'
alias gaa='git add --all'
alias gc='git commit -a'
alias gm='git commit -m'

alias t='cd /tmp'
